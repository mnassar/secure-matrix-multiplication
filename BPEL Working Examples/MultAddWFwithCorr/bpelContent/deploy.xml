<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:MatrixService="http://www.example.org/MatrixService/" xmlns:matrixop.tps="http://matrixop.tps">
  <process name="matrixop.tps:multaddProcess">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="matrixop.tps:multaddProcessService" port="multaddProcessPort"/>
    </provide>
    <invoke partnerLink="MatrixOP_PL">
      <service name="MatrixService:MatrixOperationService" port="matrixPort"/>
    </invoke>
  </process>
</deploy>