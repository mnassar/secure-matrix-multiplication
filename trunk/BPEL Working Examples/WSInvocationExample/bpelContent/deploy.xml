<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:pingpong.bpel.tps="http://pingpong.bpel.tps" xmlns:wsinvocation.tps="http://wsinvocation.tps">
  <process name="wsinvocation.tps:WS_InvokationProcess">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="wsinvocation.tps:WS_InvokationProcessService" port="WS_InvokationProcessPort"/>
    </provide>
    <invoke partnerLink="PingPongPL">
      <service name="pingpong.bpel.tps:PingPongService" port="PingPong"/>
    </invoke>
  </process>
</deploy>