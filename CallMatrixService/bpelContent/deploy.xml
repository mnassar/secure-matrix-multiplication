<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:sample="http://eclipse.org/bpel/sample" xmlns:securepckg="http://securepckg">
  <process name="sample:MatrixBPEL">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:MatrixBPELService" port="MatrixBPELPort"/>
    </provide>
    <invoke partnerLink="MatService_PL">
      <service name="securepckg:MatrixServiceService" port="MatrixService"/>
    </invoke>
  </process>
</deploy>